{
	"mappings": [
		{
			"src": "scss",
			"dest": "./css"
		},
		{
			"src": "js",
			"dest": "./js-min"
		}
	],
	"ignores": [
		"*.css"
	],
	"options": {
		"outputStyle": "nested",
		"lineComments": false,
		"debugInfo": false,
		"sourceMap": false,
		"unixNewlines": false,
		"autoprefix": true,
		"autoprefixConfig": "> 1%, last 2 versions, Firefox ESR, Opera 12.1"
	},
	"customOptions": [],
	"includePaths": [],
	"requireLibs": []
}